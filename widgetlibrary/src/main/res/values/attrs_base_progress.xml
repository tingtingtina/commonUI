<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseProgress">
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="progress" format="integer" />
    </declare-styleable>

    <declare-styleable name="BaseCircleProgress">
        <attr name="cp_progress_width" format="dimension" />
        <attr name="cp_progress_color" format="color" />
        <attr name="cp_padding" format="dimension" />
        <attr name="cp_frame_width" format="dimension" />
        <attr name="cp_frame_color" format="color" />
        <attr name="cp_base_color" format="color" />
        <attr name="cp_base_width" format="dimension" />
        <attr name="cp_start_angle" format="float"/>
        <attr name="cp_span_angle" format="float"/>
        <attr name="cp_textSize" format="dimension"/>
        <attr name="cp_textColor" format="color"/>
        <attr name="cp_suffix_textSize" format="dimension"/>
        <attr name="cp_suffix_textColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="BaseSeekBar">
        <attr name="seek_disable_color" format="color" />
        <attr name="seek_base_color" format="color" />
        <attr name="seek_progress_color" format="color" />
        <attr name="seek_height" format="dimension" />
    </declare-styleable>
</resources>
